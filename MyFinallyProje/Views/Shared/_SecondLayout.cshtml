</head>
<body>
    <header> 
        <section id="Menu rghebvroh">
            <nav class="navbar navbar-expand-lg navbar-light">
                <div class="container-fluid">
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse  justify-content-between" id="navbarSupportedContent">
                        <ul class="navbar-nav me-auto mb-2 mb-lg-0">

                            <li class="nav-item">
                                <a class="nav-link active text-white" aria-current="page" asp-action="index" asp-controller="home">Home</a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link active  text-white" aria-current="page" asp-action="index" asp-controller="about">About</a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link active text-white" aria-current="page" asp-action="index" asp-controller="service">Event</a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link active text-white" aria-current="page" asp-action="index" asp-controller="blog">Blog</a>
                            </li>


                            <li class="nav-item">
                                <a class="nav-link active text-white" aria-current="page" asp-action="index" asp-controller="team">Team</a>
                            </li>


                            <li class="nav-item">
                                <a class="nav-link active text-white" aria-current="page" asp-action="index" asp-controller="shop">Shop</a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link active  text-white" aria-current="page" asp-action="index" asp-controller="contact">Contact</a>
                            </li>


                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle  text-white" asp-action="index" asp-controller="home" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    Account
                                </a>

                                <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                    @if (User.Identity.IsAuthenticated)
                                    {
                                        <li><a class="dropdown-item">@User.Identity.Name</a></li>
                                        <li><a class="dropdown-item" asp-action="Logout" asp-controller="account">Logout</a></li>

                                    }
                                    else
                                    {
                                        <li><a class="dropdown-item" asp-action="login" asp-controller="account">Login</a></li>
                                        <li><a class="dropdown-item" asp-action="register" asp-controller="account">Register</a></li>

                                    }


                                    <li><hr class="dropdown-divider"></li>
                                </ul>

                          
                        </ul>
                    </div>
                </div>
            </nav>

        </section>
    </header>
    @RenderBody()
    <footer class="yeniclassss mt-5">
