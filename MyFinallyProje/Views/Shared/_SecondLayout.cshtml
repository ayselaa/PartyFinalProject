                    </div>
                    <div data-aos="flip-down" class="col-md-4 footer-mid">
                        <h2>Join Us?</h2>
                    </div>
                    <div data-aos="fade-left" class="col-md-4 footer-right">
                        <button type="button" class="btn btnDig">CONTACT US</button>
                    </div>
                </div>
            </div>
        </section>
        <section id="Footer1">
            <div class="container">
                <div class="row justify-content-around">
                    <div data-aos="fade-right" class="col-md-6 col-12 footer1">
                        <h4>Baku</h4>
                        <p>+994 503456789</p>
                    </div>
                    <div data-aos="fade-left" class="col-md-6 col-12 footer1">
                        <p class="mt-4">abilovaysel@gmail.com</p>
                    </div>
                </div>
            </div>
            </div>
        </section>
        <section id="Footer3">
            <div class="container">
